{
  "name": "tobixen-inventory_md",
  "version": "0.1.0",
  "author": "tobixen",
  "summary": "Puppet module for deploying inventory-system instances",
  "license": "Apache-2.0",
  "source": "https://github.com/tobixen/puppet-inventory-md",
  "project_page": "https://github.com/tobixen/puppet-inventory-md",
  "issues_url": "https://github.com/tobixen/puppet-inventory-md/issues",
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 8.0.0 < 10.0.0"
    },
    {
      "name": "puppetlabs/vcsrepo",
      "version_requirement": ">= 5.0.0 < 7.0.0"
    }
  ],
  "operatingsystem_support": [
